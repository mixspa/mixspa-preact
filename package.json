{
  "name": "@mixspa/preact",
  "version": "0.1.0",
  "description": "This is a tools for mixspa with preact",
  "main": "lib/index.js",
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "babel": "babel src --out-dir lib --copy-files",
    "babel:watch": "watch 'yarn babel' ./src",
    "build": "yarn clean && yarn babel",
    "lint": "eslint src",
    "test": "jest --coverage --config config/jest.config.js",
    "test:watch": "watch 'yarn test' ./src",
    "start": "webpack-dev-server --config config/webpack.config.js --open"
  },
  "dependencies": {
    "@mixspa/core": "^0.4.3"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "preact": "^8.4.2",
    "preact-router": "^2.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.4",
    "classnames": "^2.2.6",
    "css-loader": "^2.1.0",
    "enzyme": "^3.7.0",
    "eslint": "^5.9.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "node-sass": "^4.9.3",
    "postcss-loader": "^3.0.0",
    "preact": "^8.4.2",
    "preact-router": "^2.6.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "watch": "^1.0.2",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mixspa/mixspa-preact.git"
  },
  "keywords": [
    "node",
    "babel",
    "jest",
    "preact",
    "eslint",
    "security",
    "circleci"
